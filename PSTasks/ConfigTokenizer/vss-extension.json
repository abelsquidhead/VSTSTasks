{
    "manifestVersion": 1,
    "id": "build-task",
    "name": "AbelSquidHead Build Tools",
    "version": "0.0.2",
    "publisher": "abelwang",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Tools for building with AbelSquidhead includes one build task for config tokenization.",
    "categories": [
        "Build and release"
    ],
    "icons": {
        "default": "images/icon.png"        
    },
    "files": [
        {
            "path": "buildtask"
        }
    ],
    "contributions": [
        {
            "id": "custom-build-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "buildtask"
            }
        }
    ]
}